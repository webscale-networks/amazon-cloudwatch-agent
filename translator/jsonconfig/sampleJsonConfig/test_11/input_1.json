{
  "logs": {
    "metrics_collected": {
      "prometheus": {
        "prometheus_config_path": "/tmp/prometheus.yaml",
        "emf_processor": {
          "metric_declaration": [
            {
              "source_labels": ["Service"],
              "regex": "nginx.*",
              "dimensions": [["Service"]]
            }
          ]
        }
      }
    },
    "force_flush_interval": 5
  }
}